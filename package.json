  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.11.0",
    "danger": "^4.0.2",
    "husky": "^1.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "prettier": "^1.9.2",
    "release-it": "^7.5.0",
    "ts-jest": "^23.10.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.2.0",
    "typescript": "^3.0.3"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "start": "node  -r dotenv/config ./node_modules/.bin/babel-node  --extensions '.ts,.js' --inspect src/index.ts"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "tslint --fix",
      "yarn prettier --write",
      "git add"
    ]
  },
  "jest": {
    "preset": "ts-jest"
  },
  "prettier": {
    "printWidth": 115,
    "semi": false,
    "singleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "proseWrap": "always"
  },
  "dependencies": {
    "@babel/core": "^7.1.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/graphql": "^14.0.1",
    "apollo-link": "^1.2.3",
    "apollo-link-context": "^1.0.9",
    "apollo-link-http": "^1.5.5",
    "apollo-server-express": "^2.1.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.0",
    "ip": "^1.1.5",
    "url-join": "^4.0.0",
    "uuid": "^3.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run build"
    }
  }
}